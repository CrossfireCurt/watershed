        },
        {
          "InstanceRole": "TASK",
          "InstanceCount": 1,
          "InstanceType": "m3.xlarge"
        }
      ],
      "hive":{
        "tableName": "AppEvent"
      },
      "kinesis": {
        "streamName": "AppEventStream"
      },
      "tags":[
        {
          "Key": "Name",
          "Value": "stream-archival"
        }
      ]
    },
    "profile": "default"
