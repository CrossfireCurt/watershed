{
  "AWS": {
    "S3": {
      "resourcesBucket": "watershed",
      "resourcesPrefix": "resources"
    },
    "EMR": {
      "clusterName": "watershed",
      "roles": {
        "service": "EMR_DefaultRole",
        "ec2": "EMR_EC2_DefaultRole"
      },
      "ec2KeyName": null,
      "ec2SubnetId": null,
      "additionalMasterSecurityGroups": [],
      "amiVersion": "3.8.0",
      "archivalDfsUrl": "s3n://AccessKey:SecretKey@watershed",
      "archivesPath": "/stream-archives",
      "archiveName": "app_event",
      "instanceGroups": [
        {
          "InstanceRole": "MASTER",
          "Name": "MASTER",
          "InstanceCount": 1,
          "InstanceType": "m3.xlarge"
        },
        {
          "InstanceRole": "CORE",
          "Name": "CORE",
          "InstanceCount": 2,
          "InstanceType": "m3.xlarge"
        },
        {
          "InstanceRole": "TASK",
          "Name": "TASK",
          "InstanceCount": 1,
          "InstanceType": "m3.xlarge"
        }
      ],
      "hive":{
        "tableName": "app_event_stream"
      },
      "kinesis": {
        "streamName": "app_event"
      },
      "tags":[
        {
          "Key": "Name",
          "Value": "watershed"
        }
      ]
    },
    "profile": "default"
  }
}
